        "firstName",
        "academicYear",
        "class",
        "schoolName",
        "schoolUniqueCode",
        "issuanceDate",
        "certificateEnrollmentId"
      ],
      "uniqueIndexFields": ["studentId"],
      "properties": {
        "studentId": {
          "type": "string",
          "title": "Student Identifier"
        },
        "schoolId": {
          "type": "string",
          "title": "School Identifier"
        },
        "firstName": {
          "type": "string",
          "title": "First Name"
        },
        "middleName": {
          "type": "string",
          "title": "Middle Name"
        },
        "lastName": {
          "type": "string",
          "title": "Last Name"
        },
        "academicYear": {
          "type": "string",
          "title": "Academic Year"
        },
        "class": {
          "type": "string",
          "title": "Class"
        },
        "schoolUniqueCode": {
          "type": "string",
          "title": "School Unique Code"
        },
        "schoolAddressLine1": {
          "type": "string",
          "title": "School Address Line 1"
        },
        "schoolAddressLine2": {
          "type": "string",
          "title": "School Address Line 2"
        },
        "landmark": {
          "type": "string",
          "title": "Landmark"
        },
        "locality": {
          "type": "string",
          "title": "Locality"
        },
        "district": {
          "type": "string",
          "title": "District"
