        },
        "state": {
          "type": "string",
          "title": "State"
        },
        "country": {
          "type": "string",
          "title": "Country"
        },
        "studentStatus": {
          "type": "string",
          "title": "Student Status"
        },
        "schoolName": {
          "type": "string",
          "title": "School Name"
        },
        "authorityName": {
          "type": "string",
          "title": "Authority Name"
        },
        "principalDesignation": {
          "type": "string",
          "title": "Principal Designation"
        },
        "validUpto": {
          "type": "string",
          "format": "date",
          "title": "Valid Upto"
        },
        "issuanceDate": {
          "type": "string",
          "format": "date",
          "title": "Issuance Date"
        },
        "certificateEnrollmentId": {
          "type": "string",
          "title": "Certificate Enrollment ID"
